[ignore]
<PROJECT_ROOT>/node_modules/react-side-effect/.*
<PROJECT_ROOT>/immutable/dist/.*

[include]
<PROJECT_ROOT>/src/.*

[libs]

[lints]
all=error
untyped-import=warn
unclear-type=warn
sketchy-null=off
unsafe-getters-setters=warn

[options]
include_warnings=false
esproposal.optional_chaining=enable

[strict]
